inline boolean zero_prop(word load_id, word amps)
{
  if(amps > AMP_SERVICE)
    return true;
    
  return (PMF_D[amps][load_id] == 0);
}

inline boolean zero_unprop(word load_id, word amps)
{
  if(amps > AMP_SERVICE)
    return true;

  return (PMF_U[amps][load_id] == 0);
}

inline double get_prop(word load_id, word amps)
{
  return (double)PMF_D[amps][load_id] / (double)SAMPLES_COUNT;
}

inline double get_unprop(word load_id, word amps)
{
  return (double)PMF_U[amps][load_id] / (double)SAMPLES_COUNT;
}

inline word get_load_count()
{
  return LOAD_COUNT;
}

inline const char *get_load_name(word load_id)
{
  return LOADS[load_id][1];
}
